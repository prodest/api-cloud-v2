    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/build/bin/www",
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Index",
            "program": "${workspaceRoot}/index.js",
            "args": [
                "--SERVICE_ID=1s75"
            ],
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}